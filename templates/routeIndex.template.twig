import config from '../config/constants';
{% for route in routes %}
import {{ route.name|lower }}Routes from './{{ route.module|lower }}/routes/{{ route.name|lower }}.routes';
{% endfor %}

export default (app) => {
{% for route in routes %}
  app.use(`${config.ROOT_URL}{{ route.url }}`, {{ route.name|lower }}Routes);
{% endfor %}
};
